{
  "name": "bring-n-ring-site",
  "description": "Website for brin n ring",
  "version": "1.0.0",
  "private": true,
  "author": "Bring&Ring <developer@bringandring.de >",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "dev": "gatsby develop",
    "prettier": "prettier --write \"**/*.{ts,tsx,md,yml,yaml}\"",
    "lint": "eslint '**/*.{ts,tsx,js}'",
    "start": "npm run dev",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc",
    "postinstall": "patch-package"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "lint-staged": {
    "*.js": [
      "npm run prettier && npm run lint"
    ],
    "*.jsx": [
      "npm run prettier && npm run lint"
    ],
    "*.ts": [
      "npm run prettier && npm run lint"
    ],
    "*.tsx": [
      "npm run prettier && npm run lint"
    ],
    "*": [
      "npm run prettier"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@bring-n-ring/components": "0.0.11",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@ionic/react": "^5.0.7",
    "babel-plugin-styled-components": "^1.10.7",
    "classnames": "^2.2.6",
    "gatsby": "^2.20.9",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-canonical-urls": "^2.2.1",
    "gatsby-plugin-emotion": "^4.2.1",
    "gatsby-plugin-pathdata": "^1.1.0",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-sharp": "^2.5.3",
    "gatsby-plugin-typescript": "^2.3.1",
    "gatsby-remark-copy-linked-files": "^2.2.1",
    "gatsby-remark-images": "^3.2.1",
    "gatsby-remark-prismjs": "^3.4.1",
    "gatsby-remark-responsive-iframe": "^2.3.1",
    "gatsby-remark-smartypants": "^2.2.1",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-source-instagram": "^0.7.0",
    "gatsby-transformer-json": "^2.3.1",
    "gatsby-transformer-remark": "^2.7.1",
    "gatsby-transformer-sharp": "^2.4.3",
    "ionicons": "^5.0.1",
    "lottie-web": "^5.6.7",
    "modern-normalize": "^0.6.0",
    "normalize.css": "^8.0.1",
    "polished": "^3.5.1",
    "prism-themes": "^1.3.0",
    "prismjs": "^1.19.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-intl": "^4.3.1",
    "terser": "3.14.1",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/node": "^13.9.5",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@types/react-intl": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "eslint-config-blvd": "^1.0.2",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "gatsby-plugin-google-gtag": "^1.2.1",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.3",
    "install": "^0.13.0",
    "lint-staged": "^10.1.0",
    "patch-package": "^6.2.1",
    "prettier": "^2.0.2",
    "rimraf": "^3.0.2"
  }
}
